{
    "name": "d7c_nodejs_gulp",
    "version": "1.0.0",
    "description": "一个使用 npm 和 bower 进行包管理，gulp 进行自动化构建的快速 web 骨架工程。",
    "scripts": {
        "prestart": "npm install && bower install",
        "postinstall": "gulp compile",
        "start": "./node_modules/.bin/hs -a 127.0.0.1 -p 8080 -o /dist",
        "pretest": "gulp compile",
        "test": "./node_modules/.bin/browser-sync start --server dist --files dist --browser chrome --host 127.0.0.1 --port 8080 --startPath index.html --no-notify"
    },
    "repository": {
        "type": "git",
        "url": "https://gitee.com/d7c/d7c_nodejs_gulp.git"
    },
    "keywords": [
        "nodejs",
        "gulp",
        "d7c",
        "第七仓",
        "吴佳隆"
    ],
    "author": "吴佳隆",
    "license": "AGPL-3.0",
    "dependencies": {},
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.3",
        "babel-preset-env": "^1.7.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-runtime": "^6.26.0",
        "browser-sync": "^2.26.12",
        "del": "^5.1.0",
        "gulp": "^4.0.2",
        "gulp-babel": "^7.0.1",
        "gulp-clean-css": "^4.3.0",
        "gulp-concat": "^2.6.1",
        "gulp-cssnano": "^2.1.3",
        "gulp-htmlmin": "^5.0.1",
        "gulp-less": "^4.0.1",
        "gulp-rename": "^2.0.0",
        "gulp-uglify": "^3.0.2"
    }
}
